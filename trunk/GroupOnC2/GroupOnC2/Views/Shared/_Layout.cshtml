        function purchase(dealAlias, free) {
            if (free == "False") {
                LoginAndRegisterFunction.LoginDone = function (v) {
                    location.href = "Details/OrderInformation".replace("Details", dealAlias);
                };
                LoginAndRegisterFunction.RegisterDone = function (v) {
                    location.href = "Details/OrderInformation".replace("Details", dealAlias);
                };
                LoginAndRegisterFunction.ForgotPasswordDone = function (v) {
                    location.href = location.pathname;
                };

                LoginAndRegisterFunction.ShowWindow(1);
            }
            else {
                location.href = "Details/OrderInformation".replace("Details", dealAlias);
            }

        }


        function purchase1(free) {
